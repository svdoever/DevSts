@using System.Security.Claims
@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>

<p>Claims:</p>
<ul>
    @foreach (var claim in ((ClaimsIdentity) User.Identity).Claims)
    {
        <li>@claim.Type: @claim.Value</li>
    }
</ul>